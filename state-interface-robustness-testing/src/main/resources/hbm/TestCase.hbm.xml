<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping>
    <class name="robtest.stateinterfw.hbm.TestCase" table="TEST_CASE">
        <id name="id" type="int" column="id">
            <generator class="native" />
        </id>
        <set name="testInputs" inverse="true" lazy="true">
            <key column="test_id" />
            <one-to-many class="robtest.stateinterfw.hbm.TestInput" />
        </set>
    </class>
</hibernate-mapping>