<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping>
    <class name="robtest.stateinterfw.data.TestCase" table="TEST_CASE">
        <id name="id" type="int" column="id">
            <generator class="native" />
        </id>
        <set name="testInputs" inverse="true" lazy="true">
            <key column="test_id" />
            <one-to-many class="robtest.stateinterfw.data.TestInput" />
        </set>
        <property name="uniqueIdentifier" type="text" column="uid" />
    </class>
</hibernate-mapping>